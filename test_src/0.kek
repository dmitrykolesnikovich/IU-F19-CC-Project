class Boolean is
    var value : Boolean
end

class Integer is
    var value : Integer
end

class Real is
    var value : Real
end

class Animal is

    var iq : Integer
	var alive : Boolean
    var age : Integer
    var weight : Real
    var sex : Boolean

	this (iq_p : Integer, alive_p : Boolean, age_p : Integer, weight_p : Real, sex_p : Boolean) is
    	iq := iq_p
        alive := alive_p
        age := age_p
        weight := weight_p
        sex := sex_p
        return this
    end
end

class Human extends Animal is
	var knowLanguage : true
    var status : 0
    this (iq_p : Integer, alive_p : Boolean, age_p : Integer, weight_p : Real, sex_p : Boolean, knowLanguage_p : Boolean, status_p : Integer) is
        iq := iq_p
        alive := alive_p
        age := age_p
        weight := weight_p
        sex := sex_p
        status := status_p
        knowLanguage := knowLanguage_p
        return this
    end
end


